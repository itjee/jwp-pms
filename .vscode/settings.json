{
    "cSpell.words": [
      "aiofiles",
      "asyncio",
      "asyncpg",
      "datefmt",
      "dateutil",
      "dotenv",
      "fastapi",
      "httpx",
      "isort",
      "levelname",
      "loguru",
      "newgrp",
      "NOTSET",
      "onupdate",
      "passlib",
      "psql",
      "psycopg",
      "pydantic",
      "pytest",
      "qualname",
      "repr",
      "sqlalchemy",
      "structlog",
      "usermod",
      "uvicorn",
      "venv"
    ],
    "editor.formatOnSave": true,
    "python.formatting.provider": "blackArgs",
    "python.sortImports.onSave": true,
    "python.analysis.diagnosticSeverityOverrides": {
    "reportMissingTypeStubs": "none"
  },
  "python.linting.flake8Enabled": true,
  "python.linting.enabled": true,
  "python.linting.flake8Args": [
    "--max-line-length=100",
    "--extend-ignore=E203,W503"
  ]
}